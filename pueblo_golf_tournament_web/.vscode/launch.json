{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Flutter for web",
			"type": "dart",
			"request": "launch",
			"program": "lib/main.dart",
			"args": ["-d", "chrome", "--web-browser-flag", "--disable-web-security"]
		},
		{
			"name": "Launch Edge",
			"request": "launch",
			"type": "msedge",
			"url": "http://localhost:8080",
			"webRoot": "${workspaceFolder}"
		},
		{
			"name": "pueblo_golf_tournament_web",
			"request": "launch",
			"type": "dart"
		},
		{
			"name": "pueblo_golf_tournament_web (profile mode)",
			"request": "launch",
			"type": "dart",
			"flutterMode": "profile"
		},
		{
			"name": "pueblo_golf_tournament_web (release mode)",
			"request": "launch",
			"type": "dart",
			"flutterMode": "release"
		}
	]
}
